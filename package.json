{
  "name": "ethereum-arbitrage-tool",
  "version": "1.0.0",
  "description": "Ethereum Flash Loan Arbitrage Bot",
  "main": "src/main.rs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy-flashloan.js --network sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy-mainnet.js --network mainnet",
    "interact": "npx hardhat run scripts/interact-flashloan.js --network sepolia",
    "verify": "npx hardhat run scripts/verify-contract.js --network sepolia",
    "execute": "npx hardhat run scripts/execute-arbitrage.js --network sepolia",
    "execute:mainnet": "npx hardhat run scripts/execute-small-capital.js --network mainnet",
    "bot:start": "npx hardhat run scripts/continuous-arbitrage.js --network mainnet",
    "bot:mev-boost": "npx hardhat run scripts/mev-boost-integration.js --network mainnet",
    "test:arbitrage": "npx hardhat run scripts/test-arbitrage.js --network sepolia",
    "monitor:mainnet": "npx hardhat run scripts/monitor-profits.js --network mainnet",
    "clean": "npx hardhat clean"
  },
  "keywords": [
    "ethereum",
    "arbitrage",
    "flash-loan",
    "defi",
    "blockchain"
  ],
