  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "hardhat": "^2.25.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.1",
    "axios": "^1.10.0",
    "dotenv": "^16.3.1",
    "eventsource": "^4.0.0",
    "ws": "^8.18.2"
  }
